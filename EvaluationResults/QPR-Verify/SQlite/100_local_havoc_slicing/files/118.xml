  <Line number="16"> */</Line>
  <Line number="17"></Line>
  <Line number="18">/*</Line>
  <Line number="19"> * This macro may have been defined in &lt;gnu/types.h&gt;. But we always</Line>
  <Line number="20"> * use the one here.</Line>
  <Line number="21"> */</Line>
  <Line number="22">#undef __FD_SETSIZE</Line>
  <Line number="23">#define __FD_SETSIZE	1024</Line>
