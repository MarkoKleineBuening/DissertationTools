  <Line number="19"></Line>
  <Line number="20">#ifndef _GETOPT_POSIX_H</Line>
  <Line number="21">#define _GETOPT_POSIX_H 1</Line>
  <Line number="22"></Line>
  <Line number="23">#if !defined _UNISTD_H &amp;&amp; !defined _STDIO_H</Line>
  <Line number="24">#error &quot;Never include getopt_posix.h directly; use unistd.h instead.&quot;</Line>
  <Line number="25">#endif</Line>
  <Line number="26"></Line>
  <Line number="27">#include &lt;bits/getopt_core.h&gt;</Line>
  <Line number="28"></Line>
  <Line number="29">__BEGIN_DECLS</Line>
  <Line number="30"></Line>
  <Line number="31">#if defined __USE_POSIX2 &amp;&amp; !defined __USE_POSIX_IMPLICITLY \</Line>
  <Line number="32">    &amp;&amp; !defined __USE_GNU &amp;&amp; !defined _GETOPT_H</Line>
  <Line number="33">/* GNU getopt has more functionality than POSIX getopt.  When we are</Line>
  <Line number="34">   explicitly conforming to POSIX and not GNU, and getopt.h (which is</Line>
  <Line number="35">   not part of POSIX) has not been included, the extra functionality</Line>
  <Line number="36">   is disabled.  */</Line>
  <Line number="37"># ifdef __REDIRECT</Line>
  <Line number="38">extern int __REDIRECT_NTH (getopt, (int ___argc, char *const *___argv,</Line>
  <Line number="39">				    const char *__shortopts),</Line>
  <Line number="40">			   __posix_getopt);</Line>
  <Line number="41"># else</Line>
  <Line number="42">extern int __posix_getopt (int ___argc, char *const *___argv,</Line>
  <Line number="43">			   const char *__shortopts)</Line>
  <Line number="44">  __THROW __nonnull ((2, 3));</Line>
  <Line number="45">#  define getopt __posix_getopt</Line>
  <Line number="46"># endif</Line>
  <Line number="47">#endif</Line>
  <Line number="48"></Line>
  <Line number="49">__END_DECLS</Line>
  <Line number="50"></Line>
  <Line number="51">#endif /* getopt_posix.h */</Line>
